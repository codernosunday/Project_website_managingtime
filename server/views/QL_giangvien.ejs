<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Quản lý giảng viên</title>
    <link rel="icon" href="LOGO_TIMING_MAIN.ico" type="image/x-icon">
    <link rel="stylesheet" type="text/css" href="/css/navbar.css" />
    <link rel="stylesheet" type="text/css" href="/css/css_style_main.css" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"></script>
</head>

<body>
    <div class="topnav" id="myTopnav">
        <a href="/"><span id="icon_navbar2"></span></a>
        <a href="/userhome"><i class="bi bi-house"></i> Home</a>
        <a href="/manage"><i class="bi bi-clipboard"></i> Công cụ quản lý</a>
        <a href="/exeltool"><i class="bi bi-file-earmark-ruled"></i> Làm việc với Exel</a>
        <div id="user_infor"><a href="#infor"><i class="bi bi-person-circle" style="font-size: 25px;"></i></a></div>
        <a href="javascript:void(0);" class="icon" onclick="myFunction()">
            <i class="bi bi-three-dots" style="font-size: 25px;"></i>
        </a>
    </div>
    <div class="row">
        <div class="col-sm-6 p-3 text-white" style="overflow: auto;height: 250px;">
            <table class="table_css">
                <tr>
                    <th>Mã số</th>
                    <th>Tên GV</th>
                </tr>
                <% giangvien.forEach(gv=> { %>
                    <tr>
                        <td>
                            <%= gv.id %>
                        </td>
                        <td>
                            <%= gv.hoten %>
                        </td>
                    </tr>
                    <% }); %>
            </table>

        </div>

        <div class="col-sm-4 p-4  text-black">
            <form action="/action_page.php">
                <fieldset>
                    <legend>Personalia:</legend>
                    <label for="fname">First name:</label>
                    <input type="text" id="fname" name="fname"><br><br>
                    <label for="lname">Last name:</label>
                    <input type="text" id="lname" name="lname"><br><br>
                    <label for="email">Email:</label>
                    <input type="email" id="email" name="email"><br><br>
                    <label for="birthday">Birthday:</label>
                    <input type="date" id="birthday" name="birthday"><br><br>
                    <input type="submit" value="Submit">
                </fieldset>
            </form>
        </div>
    </div>
</body>
<script src="/script/navbar_script.js"></script>

</html>