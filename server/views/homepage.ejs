<html lang="en">

<head>
    <link rel="icon" href="LOGO_TIMING_MAIN.ico" type="image/x-icon">
    <link rel="stylesheet" type="text/css" href="./css/navbar.css" />
    <link rel="stylesheet" type="text/css" href="./css/css_homepage.css" />
    <link rel="stylesheet" type="text/css" href="/css/css_style_main.css" />
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"></script>

    <title>Timing</title>
</head>

<body>
    <div class="navbar">
        <a href="/"><span id="icon_navbar"></span></a>
        <a id="login_a" href="./login"><span><i class="bi bi-arrow-right-square-fill"></i></span> Đăng nhập</a>
    </div>
    <div class="container p-5 my-5" style="text-align: center;">
        <h1 style="color: 1c00f4;">Website tính giờ giảng thực tế của giảng viên</h1>
        <div style="margin-left: 25%;width: 50%;">
            <select class="form-select" aria-label="Default select example" style="text-align: center;"
                id="select_HocKy">
                <% hocky.forEach((option)=> { %>
                    <option value="<%= option.ID_HK %>">
                        <%= option.MaHK %> - <%= option.TenHocKy %> - <%= option.NamHoc %>
                    </option>
                    <% }); %>
            </select>
        </div>
        <div style="padding: 20px;"><b>Họ tên giảng viên hoặc user ID: </b><input type="text" placeholder="Nguyễn Văn A"
                id="hotengv"> </div>
        <button style="background-color: #1c00f4;" type="button" class="btn btn-primary" id="btn_kt"
            onclick="search_in_homepage()"><i class="bi bi-search"></i>
            Check</button>
        <div id="note" class="container p-12 my-3"></div>
        <div id="table-container" class="container p-12 my-5" style="margin: 8px;">

        </div>

    </div>
</body>
<script src="/script/homepage_script.js"></script>
<footer class="text-center">
    <div style="text-align: center;">
        <div style="padding: 10px;">
            <b>Đăng ký ngay </b>
            <button type="button" class="btn btn-outline-dark btn-rounded">
                Sign up!
            </button>
        </div>
        <div>© 2024 Copyright: <a href="#33" style="text-decoration: none;">Đồ án 1 khoa Công Nghệ Thông Tin Đại Học Sư
                Phạm Kỹ Thuật Vĩnh Long</a>
        </div>
    </div>
</footer>

</html>